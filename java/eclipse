#!/bin/bash

. common

case $1 in
  start)
    start eclipse $homedir/java/eclipse/eclipse
    ;;
  stop)
    stop eclipse
    ;;
  remove)
    remove eclipse
    ;;
  shell)
    shell eclipse
    ;;
  status)
    status eclipse
    ;;
  logs)
    logs eclipse
    ;;
  "")
    start eclipse $homedir/java/eclipse/eclipse
    ;;
  *)
    usage
    ;;
esac
